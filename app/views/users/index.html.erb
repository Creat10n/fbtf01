<% provide :title, t(".title") %>
<h1><%= t ".title" %></h1>
<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user_path(user) %>
      <% unless user.admin? %>
        | <%= link_to t(".delete"), user, method: :delete,
          data: {confirm: t(".sure")} %>
      <% end %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
